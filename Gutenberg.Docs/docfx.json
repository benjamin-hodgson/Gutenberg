{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "Gutenberg/bin/Release/net6.0/Gutenberg.dll"
                    ],
                    "src": ".."
                }
            ],
            "dest": "."
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "*.yml",
                    "index.md"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadata": {
            "_enableSearch": true,
            "_disableBreadcrumb": true,
            "_appTitle": "Gutenberg"
        },
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ]
    }
}
